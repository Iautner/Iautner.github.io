{
  "id": "/",
  "name": "Lautner",
  "short_name": "Lautner",
  "description": "Lautner's links and socials, Fun Minecraft content, modded Minecraft, let's play, and challenge videos!",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "orientation": "any",
  "lang": "en",
  "dir": "auto",
  "theme_color": {
    "light": "#007AFF",
    "dark": "#0A84FF"
  },
  "background_color": {
    "light": "#F2F2F7",
    "dark": "#000000"
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "icons": [
    {
      "src": "/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "monochrome"
    }
  ],
  "screenshots": [
    {
      "src": "/Screenshot.png",
      "sizes": "2880x1800",
      "type": "image/png",
      "description": "A screenshot of the home page",
      "form_factor": "wide"
    }
  ],
  "categories": ["entertainment", "social", "games"],
  "shortcuts": [
    {
      "name": "Quick Links",
      "short_name": "Links",
      "description": "Access social links directly",
      "url": "/#links",
      "icons": [
        {
          "src": "/manifest-icon-192.maskable.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+lautner",
      "url": "/?action=%s"
    }
  ],
  "share_target": {
    "action": "/?share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/?open",
      "accept": {
        "text/plain": [".txt"],
        "application/json": [".json"]
      },
      "icons": [
        {
          "src": "/manifest-icon-192.maskable.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
  "capture_links": "existing-client-navigate",
  "display_mode": "fullscreen",
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "prefer_related_applications": false
}